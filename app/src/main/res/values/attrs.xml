<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MarqueeViewStyle">
        <attr name="mvInterval" format="integer|reference" />
        <attr name="mvAnimDuration" format="integer|reference" />
        <attr name="mvTextSize" format="dimension|reference" />
        <attr name="mvTextColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="FigureImageView">
        <attr name="mode" format="enum">
            <enum name="circle" value="0"/>
            <enum name="round_rect" value="1"/>
            <enum name="sector" value="2"/>
            <enum name="ring" value="3"/>
        </attr>
        <attr name="radius" format="dimension"/>
        <attr name="angle" format="float"/>
        <attr name="length" format="dimension"/>
    </declare-styleable>

</resources>